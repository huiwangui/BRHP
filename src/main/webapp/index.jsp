<script type="text/javascript">
getdetail=function(id){
	window.location.href="/BRHP/healthEdu/detail?id="+id;
	
}
  $(function(){
	  $.ajax({
			 type:'GET',
			 url:'/BRHP/orgHospital/getHospitalDetail',
			 success: function(data){
			    //停诊公告
	            var Code=["","一","二","三"]
	            var grade=["","特","甲","乙等"]
	            console.log(data)
	            $('h4:eq(0)').html(data[0].orgName)
	            var c1=data[0].levelCode
	            var g1=data[0].gradeCode
	            $('.grade:eq(0) span').html(Code[c1]+grade[g1])
	             $('.margin span:eq(0)').html(data[0].deptName)
	            $('.margin span:eq(1)').html(data[0].staffName)
	            $('.margin span:eq(2)').html(data[0].stopReason)
	            
	            if(data.length>0){
	            	$('h4:eq(1)').html(data[1].orgName)
		            var c2=data[1].levelCode
		            var g2=data[1].gradeCode
		            $('.grade:eq(1) span').html(Code[c2]+grade[g2])
		             $('.margin2 span:eq(0)').html(data[1].deptName)
		            $('.margin2 span:eq(1)').html(data[1].staffName)
		            $('.margin2 span:eq(2)').html(data[1].stopReason)
	           
	            }
			 }   
		 })
		 //查询咨询信息
		 $.ajax({
			   type:'GET',
			   async: false,
			   url:'/BRHP/healthEdu/byModMore',
			   data:{
				     pageNo:1,
					 pageSize:5,
					 moduleId:1
					 },
			 success: function(data){
			    if(data==''){}
			    var str ='';
			    for(i=0;i<data.length;i++){
			    str=str+'<div>'+
			    '                <a href="javascript:;" onclick="getdetail('+data[i].id+')">'+
			    '                    <p class="left">'+data[i].title+'</p>'+
			    '                    <p class="date">'+data[i].createTimeStr+'</p>'+
			    '                </a>'+
			    ' </div>';
			    }
			    //
			    document.getElementById('zixun').innerHTML=str;
			 }   
		 })
		  //通知公告
		 $.ajax({
			   type:'GET',
			   async: false,
			   url:'/BRHP/healthEdu/byModMore',
			   data:{
				     pageNo:1,
					 pageSize:5,
					 moduleId:6
					 },
			 success: function(data){
			    if(data==''){}
			    var str ='';
			    for(i=0;i<data.length;i++){
			    str=str+'<div>'+
			    '                <a href="javascript:;" onclick="getdetail('+data[i].id+')">'+
			    '                    <p class="left">'+data[i].title+'</p>'+
			    '                </a>'+
			    ' </div>';
			    }
			    //
			    document.getElementById('notice').innerHTML=str;
			 }   
		 })
	  
  })
</script>
